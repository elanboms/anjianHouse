.box{
  box-sizing: border-box;
  padding: 40rpx;
}
.list-section{
  border: 1rpx solid #ededed;
  box-shadow: 0rpx 5rpx 15rpx #ededed;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding: 30rpx;
  margin-bottom: 40rpx;
  font-size: 28rpx;
  display: flex;
  align-items: center;
}
.list-section-img{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  flex-shrink: 0;
  margin-right: 20rpx;
}
.list-section-tag{
  width: 130rpx;
  height: 60rpx;
  border-radius: 30rpx;
  border: 1rpx solid #0cc2c3;
  color: #0cc2c3;
  display: flex;
  justify-content: center;
  align-items: center;
}
.list-section-tag image{
  width: 26rpx;
  flex-shrink: 0;
  margin-right: 10rpx;
}
.list-section-name{
  width: 140rpx;
}
.list-section-tag .list-section-text{
  font-size: 24rpx;
}
.list-section-tag-1{
  margin-left: 20rpx;
}
.list-section-tag-2{
  margin-left: 20rpx;
  border: 1rpx solid #3295ea;
}
.list-section-tag-2 .list-section-text{
  color: #3295ea;
}
.tit-more{
  width: 14rpx;
  height: 24rpx;
  margin-left: 28rpx;
}
.hbContainer{
  position: fixed;
  right: 0rpx;
  bottom: 400rpx;
}
.hbContainer .hbImg{
  width: 173rpx;
  height: 148rpx;
  animation: hb 8s linear infinite;
}
@keyframes hb{
  0%{transform: scale(1) rotate(0);}
  5%{transform: scale(1.05) rotate(5deg);}
  10%{transform: scale(1.05) rotate(-5deg);}
  15%{transform: scale(1.05) rotate(5deg);}
  20%{transform: scale(1.05) rotate(-5deg);}
  35%{transform: scale(1.05) rotate(5deg);}
  30%{transform: scale(1.05) rotate(-5deg);}
  40%{transform: scale(1) rotate(0);}
  100%{transform: scale(1) rotate(0);}
}
.hbText{
  position: fixed;
  bottom: 550rpx;
  right: -500rpx;
  background: #ff0000;
  padding: 0 20rpx;
  border-radius: 3px;
  font-size: 24rpx;
  color: #fff;
  animation: hbtext 5s linear infinite;
}
.hbText text{
  max-width: 140rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
@keyframes hbtext{
  0%{right: -500rpx;}
  20%{right: 10rpx;}
  80%{right: 10rpx;}
  100%{right: -500rpx;}
}





@import '/packageB/pages/cards-template/cards-template.wxss';
page{
  background: #fff;
}
scroll-view{
  height: calc(100vh - 50px);
}

.peopleInfo{
  box-sizing: border-box;
  padding: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.peopleView{
  display: flex;
  flex-direction: column;
}
.peopleView view{
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-right: 30rpx;
  margin-bottom: 10rpx;
}
.peopleView view image{
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  margin-right: 5rpx;
}
.peopleView text{
  font-size: 24rpx;
  color: #888;
}
.peopleBtn{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.peopleBtn button{
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 22rpx;
  color: #888;
}
.peopleBtn button:last-child{
  margin-left: 50rpx;
}
.peopleBtn button image{
  width: 36rpx;
  height: 36rpx;
  margin-bottom: 16rpx;
}

.cardBtn{
  box-sizing: border-box;
  padding: 0 30rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.cardBtn form{
  display: block;
  flex: 1;
  overflow: hidden;
  margin-left: 30rpx;
}
.cardBtn button{
  flex: 1;
  overflow: hidden;
  line-height: 70rpx;
  border-radius: 3px;
  font-size: 32rpx;
}
.cardBtn form button{
  border: 1px solid #1AAD19;
  background: #fff;
  color: #1AAD19;
}
.cardBtn>button{
  border: 1px solid #1AAD19;
  background: #1AAD19;
  color: #fff;
}

.detailLook{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  color: #999;
  font-size: 24rpx;
  margin-right: 20rpx;
}
.detailLook image{
  width: 12rpx;
  margin-left: 15rpx;
}
.detailLook .arrowTop{
  transform: rotate(-90deg);
}
.detailLook .arrowBtm{

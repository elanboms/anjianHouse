page{
  background: #f5f5f5;
}

.title{
  box-sizing: border-box;
  padding: 30rpx;
  font-size: 28rpx;
  color: #666;
}
.tipsContent{
  display: flex;
  flex-direction: row;
  align-items: center;
  flex-wrap: wrap;
  box-sizing: border-box;
  padding: 0 30rpx;
  font-size: 28rpx;
}
.tipsContent text{
  color: #272B56;
  padding: 6rpx 20rpx;
  border: 1px solid #272B56;
  border-radius: 5rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
}

.addTips{
  display: block;
  width: 690rpx;
  line-height: 90rpx;
  margin: 0 auto;
  background: #272B56;
  color: #fff;
  font-size: 32rpx;
}


.mask{
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  width: 100%;
  background: rgba(0,0,0,0.5);
  z-index: 66;
}
.myform{
  position: fixed;
  top: 50%;
  left: 7%;
  transform: translateY(-50%);
  width: 86%;
  box-sizing: border-box;
  padding: 40rpx 30rpx;
  background: #fff;
  border-radius: 10rpx;
  z-index: 99;
}
.myform input{
  width: 100%;
  height: 80rpx;
  border: 1px solid #dedede;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding: 0 20rpx;
  margin: 40rpx 0;
}
.myform button{
  display: block;
  width: 200rpx;
  line-height: 80rpx;
  margin: 0 auto 0;
  background: #272B56;
  color: #fff;
  font-size: 32rpx;
}
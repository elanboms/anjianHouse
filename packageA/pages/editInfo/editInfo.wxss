page{
  background: #f5f5f5;
}
.section{
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 20rpx 30rpx;
  background: #fff;
  border-bottom: 1px solid #eee;
  min-height: 100rpx;
  font-size: 31rpx;
  color: #333;
}
.section view{
  display: flex;
  flex-direction: row;
  align-items: center;
}
.section view image{
  width: 90rpx;
  height: 90rpx;
}
.section view image.arrowImg{
  width: 15rpx;
  height: auto;
  margin-left: 25rpx;
}

.marginBtm{
  border: 0;
  margin-bottom: 30rpx;
}

.icon-xiangyoujiantou{
  font-size: 26rpx;
  color: #a8a8a8;
  margin-left: 10rpx;
}
